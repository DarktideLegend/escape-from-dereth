{
  "name": "efd-content",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/content/efd-content",
  "projectType": "library",
  "targets": {
    "semantic-release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "outputPath": "dist/packages/content/efd-content",
        "changelog": true,
        "github": true,
        "npm": false,
        "repositoryUrl": "https://github.com/DarktideLegend/escape-from-dereth.git",
        "preset": "conventionalcommits",
        "presetConfig": {
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "refactor",
              "section": "Code Refactoring",
              "hidden": false
            },
            {
              "type": "docs",
              "scope": "readme",
              "section": "Documentation",
              "hidden": false
            },
            {
              "type": "build",
              "scope": "deps",
              "section": "Build System Dependencies",
              "hidden": false
            },
            {
              "type": "perf",
              "section": "Performance Improvements",
              "hidden": false
            },
            {
              "type": "revert",
              "section": "Revert Changes",
              "hidden": false
            }
          ]
        },
        "releaseRules": [
          {
            "type": "docs",
            "scope": "readme",
            "release": "patch"
          },
          {
            "type": "refactor",
            "release": "patch"
          },
          {
            "type": "build",
            "scope": "deps",
            "release": "patch"
          },
          {
            "type": "revert",
            "release": "patch"
          },
          {
            "type": "perf",
            "release": "patch"
          }
        ],
        "branches": [
          "main",
          "alpha",
          "beta"
        ],
        "plugins": [
          [
            "semantic-release-discord-bot",
            {
              "packageName": "efd-content",
              "notifications": [
                {
                  "branch": "alpha/*"
                }
              ]
            }
          ]
        ],
        "tagFormat": "efd-content-v${VERSION}"
      }
    }
  },
  "tags": []
}